<div>
	{{#with main}}
	<main>
		<section>
			<div class="mb-4">{{> title data='My education' big=true}}</div>

			<ul>
				{{#each educations}}
				<li>
					{{name}}

					{{#unless isEnd}} (ще в процесі) {{/unless}}


				</li>
				{{/each}}
			</ul>
		</section>

		<section>
			<div class="mb-4">{{> title data="My certificates"}}</div>

			<ul>
				{{#each certificates}}
				<li>
					{{name}} ({{id}})
				</li>
				{{/each}}
			</ul>
		</section>
	</main>
	{{/with}}
</div>